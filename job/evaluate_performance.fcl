#include "services_microboone.fcl"
#include "messageservice.fcl"
#include "time_memory_tracker_microboone.fcl"
#include "fiducial_volume_settings.fcl"

process_name: prodeff

services:
{

    TFileService:  {fileName: "selectionPerformace.root"}
    TimeTracker:   @local::microboone_time_tracker
    MemoryTracker: @local::microboone_memory_tracker
    @table::microboone_simulation_services

} # services

physics:
{
    analyzers:
    {
        numusel:
        {

            module_type:            "EvaluatePerformance"
            FiducialVolumeSettings: @local::UBXSecFiducialVolumeSettings
            SelectionLabel: "numusel"

       }
    }

    analysis: [ numusel ]
    end_paths: [ analysis ]
} #physics

services.DetectorClocksService.InheritClockConfig: false
